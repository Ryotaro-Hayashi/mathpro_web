  <script type="text/javascript">
    // canvas要素のDOMオブジェクトを取得
    var canvas = document.getElementById("canvas1");

    // canvasの位置指定（左上基準）
    canvas.style.position = "relative";
    // canvas.style.left = "100px"; canvas.style.top = "100px"; 描画コンテキストを取得する
    var context = canvas.getContext("2d");

    var mousex = 0;
    var mouseX = 0;
    var mousey = 0;
    var mouseY = 0;

    // canvas内での軸の位置指定
    var st_X = 90;
    var st_Y = 50;

    // 傾き
    var a = 1;

    document.getElementById( "id_textBox1" ).value = a ;

    // 切片
    var b = 2 * 30;
    var c = b / 30;

    document.getElementById( "id_textBox2" ).value = c ;

    // 英語のテストの点数
    var v = 0;

    // 指定した英語の点数のX座標
    var x = 0;

    // X軸のY座標
    var y = canvas.height - st_Y;

    // アニメーションを起動させるためのカウント
    var count = 0;

    var text = "数学";

    function onButtonClick1() {
      // DOMを取得
      a = parseFloat(document.forms.id_form1.id_textBox1.value);
    }

    function onButtonClick2() {
      // DOMを取得
      b = parseFloat(document.forms.id_form2.id_textBox2.value) * 30;
    }

    function onButtonClick3() {
      // DOMを取得
      v = parseFloat(document.forms.id_form3.id_textBox3.value) * 30;
    }

    // マウスの位置を取得
    function movehandler(event) {
      // canvas基準のマウスの位置
      mousex = event.offsetX;
      mousey = event.offsetY;
    }
    window.onmousemove = movehandler;

    // マウスの位置を取得

    function en(circle_x, circle_y) {
      // 円と同じx座標と直線の交点のy座標
      var c_X = a * circle_x * 30 + parseInt(b);

      // 描画する位置を返還
      var circle_X = circle_x * 30 + st_X;
      var circle_Y = canvas.height - circle_y * 30 - st_Y;

      context.beginPath();
      context.fillStyle = "rgb(0, 0, 255)";

      context.arc(circle_X, circle_Y, 5, 0, 2 * Math.PI);

      context.fill();

      // strokeメソッドで描画コンテキストに記録されたパスをCanvasに描画
      context.stroke();
    }

    var tategaki = function (context, text, x, y) {
      var textList = text.split('\n');
      var lineHeight = context.measureText("あ").width;
      textList.forEach(function (elm, i) {
        Array.prototype.forEach.call(elm, function (ch, j) {
          context.fillText(ch, x - lineHeight * i, y + lineHeight * j);
        });
      });
    };

    function roundFloat(number, n) {
      var _pow = Math.pow(10, n);
      return Math.round(number * _pow) / _pow;
    }

    // 英語の点数を決定するボタンを押したときに実行される関数
    function prediction() {
      count = 1;
      // ボタンが押されたときにY座標を初期化して描画位置を戻す
      y = canvas.height - st_Y;
    }

    function set() {
      x = st_X + v;

      // Y座標を動かす
      y = y - 4;

      // 白でcanvasを塗る
      context.fillStyle = "rgb(255, 255, 255)";
      context.fillRect(0, 0, canvas.width, canvas.height);

      // 軸基準のマウスの位置
      mouseX = mousex - st_X;
      mouseY = canvas.height - mousey - st_Y - b;

      // 直線の傾き
      inclination = mouseY / mouseX;

      // beginPathメソッドを呼び出すことで、描画コンテキストへのパスの記録が開始され、図形を書くことができる。
      context.beginPath();
      context.strokeStyle = "rgb(0, 0, 0)";
      // moveToメソッドによって、指定した点まで移動する。 現在の点から指定した点までの線分を定義する。 直線の軌跡
      context.setLineDash([0]);
      context.moveTo(st_X, canvas.height - st_Y - b);
      context.lineTo(st_X + 100 * 3, canvas.height -  a * 100 * 3 - b - st_Y);

      context.stroke();

      en(5, 4);
      en(6, 7);
      en(7, 9);
      en(8, 6);
      en(9, 10);

      // 英語の点数を決定するボタンを押すと、
      if (count == 1) {

        context.setLineDash([2, 2]);
        context.beginPath();
        context.moveTo(x, canvas.height - st_Y);

        if(y > canvas.height - st_Y - b - a * v) {
          context.lineTo(x, y);
          context.stroke();
        } else {
          context.moveTo(x, canvas.height - st_Y);
          context.lineTo(x, canvas.height - st_Y - b - a * v);

          context.moveTo(x, canvas.height - st_Y - b - a * v);
          context.fillStyle = "rgb(255, 0, 0)";
          context.arc(x, canvas.height - st_Y - b - a * v, 5, 0, 2 * Math.PI);
          context.fill();

          context.moveTo(x, canvas.height - st_Y - b - a * v);
          context.lineTo(st_X, canvas.height - st_Y - b - a * v);

          context.arc(st_X, canvas.height - st_Y - b - a * v, 5, 0, 2 * Math.PI);
          context.fill();
        }
        context.stroke();

        context.beginPath();
        context.fillStyle = "rgb(255, 0, 0)";
        context.arc(x, canvas.height - st_Y, 5, 0, 2 * Math.PI);
        context.fill();
      }

      context.font = "bold 14px 'ＭＳ 明朝'";

      context.beginPath();
      context.fillStyle = "rgb(0, 0, 0)";

      context.fillText("2", st_X + 20 * 3 - 3, canvas.height - st_Y + 20);
      context.fillText("4", st_X + 40 * 3 - 3, canvas.height - st_Y + 20);
      context.fillText("6", st_X + 60 * 3 - 3, canvas.height - st_Y + 20);
      context.fillText("8", st_X + 80 * 3 - 3, canvas.height - st_Y + 20);
      context.fillText("10", st_X + 100 * 3 - 6, canvas.height - st_Y + 20);


      context.fillText("2", st_X - 20, canvas.height - st_Y - 20 * 3 + 5);
      context.fillText("4", st_X - 20, canvas.height - st_Y - 40 * 3 + 5);
      context.fillText("6", st_X - 20, canvas.height - st_Y - 60 * 3 + 5);
      context.fillText("8", st_X - 20, canvas.height - st_Y - 80 * 3 + 5);
      context.fillText("10", st_X - 20 - 5, canvas.height - st_Y - 100 * 3 + 5);

      context.fillText("O", st_X - 15, canvas.height - st_Y + 15);

      context.fillText("英語(点)", canvas.width / 2 - 40, canvas.height - st_Y + 40);
      tategaki(context, text, st_X - 60, canvas.height / 2);
      context.fillText("(点)", st_X - 60 - 10, canvas.height / 2 + 30);

      // if ((0 < mouseX && mousex < canvas.width - 130) && (0 < mousey && mousey < canvas.height)) { if (roundFloat(inclination, 2) == 0) { context.fillText("f(x) = " + c, parseInt(mousex) + 10, mousey); } else if (roundFloat(inclination, 2) == 1) {
      // context.fillText("f(x) = x + " + c, parseInt(mousex) + 10, mousey); } else if (roundFloat(inclination, 2) == -1) { context.fillText("f(x) = -x + " + c, parseInt(mousex) + 10, mousey); } else if (b == 0) { context.fillText("f(x) = " +
      // roundFloat(inclination, 2) + "x", parseInt(mousex) + 10, mousey); } else { context.fillText("f(x) = " + roundFloat(inclination, 2) + "x + " + c, parseInt(mousex) + 10, mousey); } } 矢印の軌跡
      context.arrow(st_X, canvas.height - st_Y, canvas.width - st_X / 2, canvas.height - st_Y, [
        0,
        1,
        -15,
        1,
        -15,
        5
      ]);
      context.arrow(st_X, canvas.height - st_Y, st_X, st_Y - 30, [
        0,
        1,
        -15,
        1,
        -15,
        5
      ]);
      context.fill();
    }

    onload = function () {
      setInterval(set, 30);
    }
  </script>

